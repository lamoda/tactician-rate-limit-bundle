{
    "name": "lamoda/tactician-rate-limit-bundle",
    "description": "Symfony bundle for lamoda/tactician-rate-limit library",
    "type": "symfony-bundle",
    "require": {
        "symfony/dependency-injection": "^3.4 || ^4.0",
        "symfony/http-kernel": "^3.4 || ^4.0",
        "symfony/config": "^3.4 || ^4.0",
        "lamoda/tactician-rate-limit": "^0.1.0",
        "symfony/yaml": "^3.4 || ^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2",
        "davedevelopment/stiphle": "^0.9.2"
    },
    "autoload": {
        "psr-4": {
            "Lamoda\\TacticianRateLimitBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lamoda\\TacticianRateLimitBundle\\Tests\\": "tests/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Pavel Agaletskiy",
            "email": "pavel.agaletskiy@lamoda.ru"
        }
    ],
    "minimum-stability": "stable",
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:lamoda/tactician-rate-limit.git"
        }
    ]
}
